html
  head
    title=title
    link(href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css', rel='stylesheet')
  body
    div.container
      div.jumbotron
        h1 #{title} page
        p
        h2 List of accounts
        ul
          p 
          each account in accounts
            li 
              a(href=account.url) #{account._id} #{account.email}
          else
            li There are no accounts.

        p
            a.btn.btn-primary(href="/createAccountForm") Create an account      
                    
                      
                    

    
